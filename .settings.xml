<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <servers>
    <server>
    <id>public-atomist-release</id>
    <username>${env.ATOMIST_REPO_USER}</username>
    <password>${env.ATOMIST_REPO_TOKEN}</password>
  </server>
    <server>
      <id>public-atomist-dev</id>
      <username>${env.ATOMIST_REPO_USER}</username>
      <password>${env.ATOMIST_REPO_TOKEN}</password>
    </server>
    <server>
      <id>atomist-release</id>
      <username>${env.ATOMIST_CI_DEPLOY_USERNAME}</username>
      <password>${env.ATOMIST_CI_DEPLOY_PASSWORD}</password>
    </server>
    <server>
      <id>atomist-dev</id>
      <username>${env.ATOMIST_CI_DEPLOY_USERNAME}</username>
      <password>${env.ATOMIST_CI_DEPLOY_PASSWORD}</password>
    </server>
  </servers>
</settings>
